	<!-- 5.5 Use Case Generiere AdresstrÃ¤ger (Seite 34) -->
	<xs:element name="GenerateLabel">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Language" type="Language" />
				<xs:element name="Envelope">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="LabelDefinition">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="LabelLayout">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:length value="2" />
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="PrintAddresses" type="PrintAddressesType" />
										<xs:element name="ImageFileType">
