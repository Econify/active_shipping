					</xs:complexType>
				</xs:element>
				<xs:element name="Errors" type="ErrorsType" minOccurs="0" />
			</xs:choice>
		</xs:complexType>
	</xs:element>

	<!-- 5.7 Use Case Lese Basisleistungen  -->
