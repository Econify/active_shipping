														</xs:simpleType>
													</xs:element>
													<xs:element name="CUS_Name2" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="25" />
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="CUS_Street">
