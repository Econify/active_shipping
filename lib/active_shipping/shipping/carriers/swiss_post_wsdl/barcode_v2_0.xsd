																									<xs:maxLength value="35" />
																								</xs:restriction>
																							</xs:simpleType>
																						</xs:element>
																						<xs:element name="REC_Country"
																							minOccurs="0">
																							<xs:simpleType>
																								<xs:restriction base="xs:string">
																									<xs:length value="2" />
																								</xs:restriction>
																							</xs:simpleType>
																						</xs:element>
																						<xs:element name="REC_Phone" minOccurs="0">
																							<xs:simpleType>
																								<xs:restriction base="xs:string">
																									<xs:maxLength value="20" />
																								</xs:restriction>
																							</xs:simpleType>
																						</xs:element>
																						<xs:element name="REC_Mobile"
																							minOccurs="0">
																							<xs:simpleType>
																								<xs:restriction base="xs:string">
																									<xs:maxLength value="20" />
																								</xs:restriction>
																							</xs:simpleType>
																						</xs:element>
																						<xs:element name="REC_EMail" minOccurs="0">
																							<xs:simpleType>
																								<xs:restriction base="xs:string">
																									<xs:maxLength value="160" />
																								</xs:restriction>
																							</xs:simpleType>
																						</xs:element>
																						<xs:element name="REC_Data" minOccurs="0">
																							<xs:complexType>
																								<xs:sequence>
																									<xs:element name="REC_DAT"
																										minOccurs="0" maxOccurs="20">
																										<xs:complexType>
																											<xs:sequence>
																												<xs:element name="REC_DAT_Type">
																													<xs:simpleType>
																														<xs:restriction base="xs:string">
																															<xs:maxLength value="35" />
																														</xs:restriction>
																													</xs:simpleType>
																												</xs:element>
																												<xs:element name="REC_DAT_Value">
																													<xs:simpleType>
																														<xs:restriction base="xs:string">
																															<xs:maxLength value="50" />
																														</xs:restriction>
																													</xs:simpleType>
																												</xs:element>
																											</xs:sequence>
																										</xs:complexType>
																									</xs:element>
																								</xs:sequence>
																							</xs:complexType>
																						</xs:element>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="Attributes" minOccurs="0">
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="ATT_PRZL" minOccurs="1"
																							maxOccurs="unbounded">
																							<xs:simpleType>
																								<xs:restriction base="xs:string">
																									<xs:maxLength value="5" />
																								</xs:restriction>
																							</xs:simpleType>
																						</xs:element>
																						<xs:element name="ATT_Amount"
																							minOccurs="0">
																							<xs:simpleType>
																								<xs:restriction base="xs:float">
																									<xs:maxInclusive value="99999.99" />
																									<xs:minInclusive value="0.00" />
																								</xs:restriction>
																							</xs:simpleType>
																						</xs:element>
																						<xs:element name="ATT_FreeText"
																							minOccurs="0">
																							<xs:simpleType>
																								<xs:restriction base="xs:string">
																									<xs:maxLength value="34" />
																								</xs:restriction>
																							</xs:simpleType>
																						</xs:element>
																						<xs:element name="ATT_DeliveryDate"
																							minOccurs="0">
																							<xs:simpleType>
																								<xs:restriction base="xs:date">
																								</xs:restriction>
																							</xs:simpleType>
																						</xs:element>
																						<xs:element name="ATT_ParcelNo"
																							minOccurs="0">
																							<xs:simpleType>
																								<xs:restriction base="xs:int">
																									<xs:maxInclusive value="99" />
