																								</xs:restriction>
																							</xs:simpleType>
																						</xs:element>
																						<xs:element name="ATT_ParcelTotal"
																							minOccurs="0">
																							<xs:simpleType>
																								<xs:restriction base="xs:int">
																									<xs:maxInclusive value="99" />
																									<xs:minInclusive value="0" />
																								</xs:restriction>
																							</xs:simpleType>
																						</xs:element>
																						<xs:element name="ATT_DeliveryPlace"
																							minOccurs="0">
																							<xs:simpleType>
																								<xs:restriction base="xs:string">
																									<xs:maxLength value="35" />
																								</xs:restriction>
																							</xs:simpleType>
																						</xs:element>
																						<xs:element name="ATT_ProClima"
																							minOccurs="0">
																							<xs:simpleType>
																								<xs:restriction base="xs:boolean">
																								</xs:restriction>
																							</xs:simpleType>
																						</xs:element>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>


